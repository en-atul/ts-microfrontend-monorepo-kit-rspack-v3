{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "tabWidth": 2,
  "semi": true,
  "useTabs": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "proseWrap": "always",
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "parser": "json-stringify"
      }
    }
  ]
}
